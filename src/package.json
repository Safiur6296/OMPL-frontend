{
  "name": "ompl-website",
  "version": "1.0.0",
  "description": "Old Malda Premier League Registration System",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev:server": "nodemon server/index.js",
    "dev:client": "live-server src --host=localhost --port=5500",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "copyfiles -u 1 \"src/**/*\" dist/",
    "clean": "rimraf dist"
  },
  "keywords": [
    "cricket",
    "tournament",
    "registration",
    "old malda"
  ],
  "author": "Md Safiur Rahaman",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "googleapis": "^126.0.1",
    "multer": "^1.4.5-lts.1",
    "razorpay": "^2.9.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "copyfiles": "^2.4.1",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.1",
    "rimraf": "^6.0.1"
  }
}
